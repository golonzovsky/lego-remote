{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": false,
        "no-console": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false,
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-leading-underscore",
                "allow-pascal-case"
            ]
        }
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/serviceWorker.ts"
        ]
    }
}
